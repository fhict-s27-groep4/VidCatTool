@model Service_Layer.ViewModels.AlgoritmSettings;

@{
    ViewData["Title"] = "Settings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@* Algorithm Settings *@
<div style="background-color: white; padding: 5%; border: 1px solid #e91e63;">
    <form method="post">
        <div>
            <span asp-validation-for="IabToleranceTier1"></span>
            <input asp-for="IabToleranceTier1" class="form-control" type="number" min="0" max="1" style="width: 11%; min-width: 100px; margin: 2%;" />
        </div>
        <div>
            <span asp-validation-for="IabToleranceTier2"></span>
            <input asp-for="IabToleranceTier2" class="form-control" type="number" min="0" max="1" style="width: 11%; min-width: 100px; margin: 2%;" />
        </div>
        @*<div><input type="number" />%</div>
        <div><input type="number" />%</div>*@
        <div><input type="submit" value="Send" class="btn" /></div>
    </form>
</div>




